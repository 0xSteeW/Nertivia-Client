{
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "extends": ["plugin:vue/essential", "eslint:recommended"],
  "plugins": ["@vue"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "off",
      {
        "args": "after-used",
        "ignoreRestSiblings": true
      }
    ]
  },
  "parserOptions": {
    "parser": "babel-eslint",

    "sourceType": "module"
  }
}
